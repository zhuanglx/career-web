<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
						"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.railway.labor.career.mapper.EvaluationPromoteMapper">

	<resultMap id="BaseResultMap" type="com.railway.labor.career.model.dto.EvaluationPromoteDTO">
		<id column="id" property="id" jdbcType="BIGINT" />
		<result column="employee_id" property="employeeId" jdbcType="BIGINT" />
		<result column="type_of_work" property="typeOfWork" jdbcType="VARCHAR" />
		<result column="sequence_old" property="sequenceOld" jdbcType="VARCHAR" />
		<result column="grade_old" property="gradeOld" jdbcType="VARCHAR" />
		<result column="star_detail" property="starDetail" jdbcType="VARCHAR" />
		<result column="star_integral" property="starIntegral" jdbcType="VARCHAR" />
		<result column="skill_detail" property="skillDetail" jdbcType="VARCHAR" />
		<result column="skill_integral" property="skillIntegral" jdbcType="VARCHAR" />
		<result column="fault_detail" property="faultDetail" jdbcType="VARCHAR" />
		<result column="fault_integral" property="faultIntegral" jdbcType="VARCHAR" />
		<result column="project_detail" property="projectDetail" jdbcType="VARCHAR" />
		<result column="project_integral" property="projectIntegral" jdbcType="VARCHAR" />
		<result column="honor_detail" property="honorDetail" jdbcType="VARCHAR" />
		<result column="honor_integral" property="honorIntegral" jdbcType="VARCHAR" />
		<result column="quasirating" property="quasirating" jdbcType="VARCHAR" />
		<result column="creator" property="creator" jdbcType="VARCHAR" />
		<result column="create_date" property="createDate" jdbcType="DATE" />
		<result column="modifier" property="modifier" jdbcType="VARCHAR" />
		<result column="modify_date" property="modifyDate" jdbcType="DATE" />
		<result column="remarks" property="remarks" jdbcType="VARCHAR" />
		<result column="del_flag" property="delFlag" jdbcType="VARCHAR" />
	</resultMap>

	<sql id="Base_Column_List">
		id,employee_id,type_of_work,sequence_old,grade_old,star_detail,star_integral,skill_detail,skill_integral
		fault_detail,fault_integral,project_detail,project_integral,honor_detail,honor_integral,quasirating,
		,creator, create_date, modifier, modify_date, remarks, del_flag
	</sql>


	<sql id="Query_Params">
		<where>
			del_flag='0'
			<if test="evaluationPromoteQuery.id!=null">
				and id=#{evaluationPromoteQuery.id}
			</if>
			<if test="evaluationPromoteQuery.employee_id!=null">
				and name=#{evaluationPromoteQuery.employeeId}
			</if>
			<if test="evaluationPromoteQuery.typeOfWork!=null and evaluationPromoteQuery.typeOfWork!=''">
				and type_of_work=#{evaluationPromoteQuery.typeOfWork}
			</if>
			<if test="evaluationPromoteQuery.sequenceOld!=null and evaluationPromoteQuery.sequenceOld!=''">
				and sequence_old=#{evaluationPromoteQuery.sequenceOld}
			</if>
			<if test="evaluationPromoteQuery.gradeOld!=null and evaluationPromoteQuery.gradeOld!=''">
				and grade_old=#{evaluationPromoteQuery.gradeOld}
			</if>
			<if test="evaluationPromoteQuery.starDetail!=null and evaluationPromoteQuery.starDetail!=''">
				and star_detail=#{evaluationPromoteQuery.starDetail}
			</if>
			<if test="evaluationPromoteQuery.starDetail!=null and evaluationPromoteQuery.starDetail!=''">
				and star_integral=#{evaluationPromoteQuery.starDetail}
			</if>
			<if test="evaluationPromoteQuery.skillDetail!=null and evaluationPromoteQuery.skillDetail!=''">
				and skill_detail=#{evaluationPromoteQuery.skillDetail}
			</if>
			<if test="evaluationPromoteQuery.skillIntegral!=null and evaluationPromoteQuery.skillIntegral!=''">
				and skill_integral=#{evaluationPromoteQuery.skillIntegral}
			</if>
			<if test="evaluationPromoteQuery.faultDetail!=null and evaluationPromoteQuery.faultDetail!=''">
				and fault_detail=#{evaluationPromoteQuery.faultDetail}
			</if>
			<if test="evaluationPromoteQuery.faultIntegral!=null and evaluationPromoteQuery.faultIntegral!=''">
				and fault_integral=#{evaluationPromoteQuery.faultIntegral}
			</if>
			<if test="evaluationPromoteQuery.projectDetail!=null and evaluationPromoteQuery.projectDetail!=''">
				and project_detail=#{evaluationPromoteQuery.projectDetail}
			</if>
			<if test="evaluationPromoteQuery.projectIntegral!=null and evaluationPromoteQuery.projectIntegral!=''">
				and project_integral=#{evaluationPromoteQuery.projectIntegral}
			</if>
			<if test="evaluationPromoteQuery.honorDetail!=null and evaluationPromoteQuery.honorDetail!=''">
				and honor_detail=#{evaluationPromoteQuery.honorDetail}
			</if>
			<if test="evaluationPromoteQuery.honorIntegral!=null and evaluationPromoteQuery.honorIntegral!=''">
				and honor_integral=#{evaluationPromoteQuery.honorIntegral}
			</if>
			<if test="evaluationPromoteQuery.quasirating!=null and evaluationPromoteQuery.quasirating!=''">
				and quasirating=#{evaluationPromoteQuery.quasirating}
			</if>
			
		</where>
	</sql>

	<sql id="Pagination">
		limit #{pagination.start}, #{pagination.limit}
	</sql>

	<select id="query" resultMap="BaseResultMap">
		SELECT
		<include refid="Base_Column_List" />
		FROM t_career_info
		<include refid="Query_Params" />
		<include refid="Pagination" />
	</select>

	<select id="count" resultType="Long">
		SELECT count(*)
		FROM t_career_info
		<include refid="Query_Params" />
	</select>

	<select id="get" resultMap="BaseResultMap">
		SELECT
		<include refid="Base_Column_List" />
		FROM t_career_info
		WHERE id = #{id, jdbcType=BIGINT} and del_flag = '0'
	</select>

	<insert id="insert" parameterType="com.railway.labor.career.model.dto.EvaluationPromoteDTO">
		INSERT INTO
		t_career_info
		(name,account,telephone,email,password,creator, create_date, modifier,
		modify_date, remarks, del_flag)
		VALUES
		(#{name},
		#{account},#{telephone},#{email}, '11', #{creator},
		#{createDate},#{modifier}, #{modifyDate},#{remarks},#{delFlag})
	</insert>

	<update id="update" parameterType="com.railway.labor.career.model.dto.EvaluationPromoteDTO">
		UPDATE
		t_career_info
		SET
		name = #{name},
		password = #{password}
		WHERE
		id = #{id} and del_flag = '0'
	</update>

	<delete id="delete" parameterType="java.lang.Long">
		UPDATE
		t_career_info
		SET
		del_flag = '1'
		WHERE
		id =#{id} and del_flag = '0'
	</delete>

</mapper>